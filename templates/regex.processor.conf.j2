{% for regex in telegraf_processors.regex %}
[[processors.regex]]
{%  for name in regex.namepass %}
{%    if loop.first %}
  namepass = [{%    endif %}"{{ name }}"{% if not loop.last %},{% else %} ]{% endif %}
{%   endfor %}

{%  for name in regex.tags %}
  [[processors.regex.tags]]
  key = "{{ name.key }}"
  pattern = "{{ name.pattern }}"
  replacement = "{{ name.replacement }}"
{%   endfor %}
{% endfor %}
